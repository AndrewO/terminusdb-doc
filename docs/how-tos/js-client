
## Using the client - guide

let client = new WOQLClient("https:127.0.0.1:6363/api", {key: "root"})
await client.connect()
await client.createDatabase("newdb", {label: "New Database", comment: "Database Created in guide", schema: true})
await client.deleteDatabase("newdb")
await client.updateTriples
await client.query
await client.branch
await client.pull
await client.push
await client.fetch
await client.rebase
await client.clonedb

await client.createGraph("newdb", {label: "New Database", comment: "Database Created in guide", schema: true})
await client.getTriples("newdb", {label: "New Database", comment: "Database Created in guide", schema: true})
await client.deleteGraph("newdb", {label: "New Database", comment: "Database Created in guide", schema: true})

module.exports = WOQLClient


